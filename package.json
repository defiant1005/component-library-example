{
  "name": "@ridiiatov/component-library-example",
  "private": false,
  "version": "0.0.2",
  "author": "Rinat Idiiatov",
  "type": "module",
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "npm": ">=11.1.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/component-library-example.umd.cjs",
  "module": "./dist/component-library-example.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/component-library.js",
      "require": "./dist/component-library.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/component-library.css"
  },
  "keywords": [
    "vue",
    "vite",
    "typescript",
    "component-library"
  ],
  "license": "ISC",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix",
    "format": "prettier --write src/",
    "st": "storybook dev -p 5002",
    "storybook": "storybook dev -p 5002 --no-open",
    "build-storybook": "storybook build",
    "type-check": "vue-tsc --noEmit",
    "stylelint": "npx stylelint \"**/*.{scss,vue}\" --fix"
  },
  "dependencies": {
    "@types/node": "^24.1.0",
    "@vueuse/core": "^13.6.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.20.0",
    "@floating-ui/dom": "^1.6.13",
    "@floating-ui/vue": "^1.1.6",
    "@storybook/addon-a11y": "^9.0.18",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-vitest": "^9.0.18",
    "@storybook/vue3-vite": "^9.0.18",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.20.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^16.0.0",
    "playwright": "^1.54.1",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.1",
    "sass": "^1.85.0",
    "storybook": "^9.0.18",
    "storybook-dark-mode": "^4.0.2",
    "stylelint": "^16.15.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.12"
  }
